<h1>記事投稿欄</h1>
<%= form_with model: @post, local: true do |form| %>
  <div>
    <%= form.label :title, "件名" %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content, "内容" %>
    <%= form.text_field :content %>
  </div>
  <div>
    <%= form.submit "投稿する" %>
  </div>
<% end %>